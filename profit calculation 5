import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
           int  x=s.nextInt() ;
            int a=s.nextInt();
            int  b=s.nextInt();
            int r=(x*a)-(x*b)-100;
    System.out.printf("%d",r);
    }
}
