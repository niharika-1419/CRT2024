#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
 #include <stdio.h>
void classify_character (char ch) 
{
    if (ch >= 'A' && ch <= 'Z')
    {
        printf("Upper\n");
    } 
    else if (ch >= 'a' && ch <= 'z') 
    {
        printf("Lower\n");
    } 
    else if (ch >= '0' && ch <= '9') 
    {
        printf("Number\n");
    } 
    else 
    {
        printf("Symbol\n");
    }
}
int main() {
    char ch;
    scanf("%c", &ch);
    classify_character(ch);
        return 0;
}
