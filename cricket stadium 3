import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int x=s.nextInt();
        int y=s.nextInt();
        int l=2*(x+y);
        int b=(x*y);
        System.out.printf("%d\n%d\n",l,b);
    }
}
